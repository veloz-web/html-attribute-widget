{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "types": ["node", "mocha", "chai", "selenium-webdriver"],
        "module": "commonjs",
        "target": "es2018",
        "noImplicitAny": true,
        "strictNullChecks": true,
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": ["node_modules/*"]
        }
    },
    "include": [
        "e2e/**/*.ts"
    ]
}
